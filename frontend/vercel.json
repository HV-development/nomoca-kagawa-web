{
  "version": 2,
  "buildCommand": "pnpm build",
  "outputDirectory": ".next",
  "installCommand": "echo '@hv-development:registry=https://npm.pkg.github.com' > .npmrc && echo \"//npm.pkg.github.com/:_authToken=${GITHUB_TOKEN}\" >> .npmrc && echo \"//npm.pkg.github.com/:always-auth=true\" >> .npmrc && pnpm install"
}
